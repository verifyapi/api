{
    "name": "verifyapi/api",
    "description": "API endpoints based on Slim",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Englick√Ω",
            "email": "jakubenglicky@gmail.com"
        }
    ],
    "require": {
	    "php": "^8.0",
        "slim/slim": "^4.7",
        "guzzlehttp/psr7": "^1.7",
        "http-interop/http-factory-guzzle": "^1.0",
        "laminas/laminas-diactoros": "^2.5",
        "nette/di": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "VerifyApi\\Api\\": "src/VerifyApi/Api"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require-dev": {
        "tracy/tracy": "^2.8",
        "phpstan/phpstan": "^0.12.81",
        "pd/coding-standard": "^1.27",
        "phpunit/phpunit": "^9"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
